{
  "extends": "airbnb-base",
  "env": {
    "browser": true
  },
  "rules": {
    "strict": 0,
    "no-console": 0,
    "global-require": 0,
    "comma-dangle": [2, "never"],
    "object-curly-spacing": [2, "never"],
    "func-names": 0,
    "arrow-parens": [2, "as-needed"],
    "import/no-extraneous-dependencies": 0,
    "import/no-dynamic-require": 0
  }
}
